%% Analyzing the n'th order Pade appoximation for delay function F(s)=e^(-ds)

d = 0.1;

% 1st Order Pade Approximation
[num1,den1] = pade(d,1);
[z1,p1,k1]=tf2zp(num1,den1);
figure(1);
pzmap(p1,z1);
title('Poles and Zeros of 1st Order Pade Approximation');

% 2nd Order Pade Approximation
[num2,den2] = pade(d,2);
[z2,p2,k2]=tf2zp(num2,den2);
